{
  "name": "eleventy-base-blog",
  "version": "5.0.2",
  "description": "A starter repository for a blog web site using the Eleventy static site generator.",
  "scripts": {
    "build": "ELEVENTY_PRODUCTION=true eleventy && NODE_ENV=production postcss tailwind/tailwind.css --o _site/style.css",
    "watch": "eleventy --watch",
    "serve": "eleventy --serve & postcss tailwind/tailwind.css --o _tmp/style.css --watch",
    "start": "eleventy --serve & postcss tailwind/tailwind.css --o _tmp/style.css --watch",
    "debug": "DEBUG=* eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy-base-blog.git"
  },
  "author": {
    "name": "Zach Leatherman",
    "email": "zachleatherman@gmail.com",
    "url": "https://zachleat.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/11ty/eleventy-base-blog/issues"
  },
  "homepage": "https://github.com/11ty/eleventy-base-blog#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "autoprefixer": "^10.0.1",
    "html-minifier": "^4.0.0",
    "luxon": "^1.21.3",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.2.5",
    "markdown-it-attrs": "^3.0.3",
    "markdown-it-bracketed-spans": "^1.0.1",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-link-attributes": "^3.0.0",
    "markdown-it-prism": "^2.1.3",
    "postcss": "^8.1.1",
    "postcss-clean": "^1.1.0",
    "postcss-cli": "^8.1.0",
    "tailwindcss": "^1.9.4"
  }
}
