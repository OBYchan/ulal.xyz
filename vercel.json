{
  "headers": [
    {
      "source": "/(thumbnail|meta|favicon|avatars)/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value" : "public, max-age=86400"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value" : "public, max-age=3600"
        }
      ]
    }
  ]
}
